{
  "extends": ["eslint:recommended"],
  "plugins": ["import"],
  "env": {
    "node": true
  },
  "parserOptions": {
    "ecmaVersion": 2021,
    "sourceType": "module"
  },  
  "rules": {
    "indent": [
      "error",
      2
    ],
    "linebreak-style": [
      "error",
      "unix"
    ],
    "quotes": [
	    "error",
	    "single"
    ],
  }
}

